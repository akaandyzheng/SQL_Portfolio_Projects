SELECT * FROM PortfolioProject..Cause_of_Death

--Convert to Percent via 2 decimal places
SELECT CAST((Cardiovascular_diseases) AS DECIMAL(10,2)) AS 'Cardiovascular_diseases%'
FROM PortfolioProject..Cause_of_Death

--Update Table with new Columns
ALTER TABLE PortfolioProject..Cause_of_Death
ADD Cardiovascular_diseases_Percentage INT;

UPDATE PortfolioProject..Cause_of_Death
SET Cardiovascular_diseases = CAST((Cardiovascular_diseases) AS DECIMAL(10,2))
UPDATE PortfolioProject..Cause_of_Death
SET Neoplasms = CAST((Neoplasms) AS DECIMAL(10,2))
UPDATE PortfolioProject..Cause_of_Death
